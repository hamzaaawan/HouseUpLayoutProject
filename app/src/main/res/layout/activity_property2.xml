    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android">

        <include
            android:id="@+id/customToolbar"
            layout="@layout/house_up_custom_headerbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_10sdp"
            android:background="@drawable/dotted_border"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/customToolbar">

            <TextView
                android:id="@+id/P2Text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Auto Fill Info"
                android:textColor="@color/colorWaterBlue"
                android:textSize="@dimen/_18ssp" />

            <TextView
                android:id="@+id/P2Text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MPAC auto populate $10.99"
                android:textColor="@color/darkGreyColor"
                android:textSize="@dimen/_18ssp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:layout_margin="@dimen/_10sdp"
            android:layout_marginTop="-15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2">
            <TextView
                android:id="@+id/P2Text3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_weight="0.6"
                android:text="Property type"
                android:textAlignment="viewStart"
                android:textColor="@color/darkGreyColor"
                android:textSize="@dimen/_14ssp" />
            <TextView
                android:id="@+id/P2Text4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_weight="0.6"
                android:text="Rental listing"
                android:textAlignment="viewStart"
                android:textColor="@color/darkGreyColor"
                android:textSize="@dimen/_14ssp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:layout_marginTop="-15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">
            <Spinner
                android:id="@+id/P2Spinner1"
                android:layout_width="0dp"
                android:layout_height="@dimen/_40sdp"
                android:layout_marginVertical="@dimen/_5sdp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_weight="0.6"
                android:background="@drawable/item_spinner"
                android:padding="@dimen/_5sdp"
                android:textColor="@color/darkGreyColor"
                android:textSize="@dimen/_12ssp" />
            <Spinner
                android:id="@+id/P2Spinner2"
                android:layout_width="0dp"
                android:layout_height="@dimen/_40sdp"
                android:layout_marginVertical="@dimen/_5sdp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.6"
                android:background="@drawable/item_spinner"
                android:padding="@dimen/_5sdp"
                android:textColor="@color/darkGreyColor"
                android:textSize="@dimen/_12ssp" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
